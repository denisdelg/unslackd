{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "object-curly-spacing": 0,
        "class-methods-use-this": 0,
        "strict": 0,
        "consistent-return": 0,
        "global-require": 0,
        "no-else-return": 0,
        "arrow-body-style": 0,
        "import/no-dynamic-require": 0,
        "object-shorthand": 0,
        "no-unused-vars": ["error", {"args": "none"}],
        "no-param-reassign": ["error", {"props": false}]
    },
    "env": {
        "jasmine": true
    }
}
